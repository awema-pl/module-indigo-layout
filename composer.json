{
    "name": "awema-pl/module-indigo-layout",
    "description": "Basic styles and components set for building user interfaces.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karol Golec",
            "email": "karolgolec10@gmail.com",
            "homepage": "https://github.pl/awema-pl/module-indigo-layout",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "karolgolec10@gmail.com"
    },
    "homepage": "https://github.com/awema-pl/module-indigo-layout",
    "keywords": [
        "Laravel",
        "IndigoLayout",
        "Awema.pl",
        "layout",
        "styles",
        "admin",
        "dashboard",
        "ui",
        "ui interface",
        "awema.pl"
    ],
    "require": {
        "php": "^7.1.3",
        "awema-pl/module-theme-switcher": "dev-master",
        "awema-pl/module-system-notify": "dev-master",
        "awema-pl/module-base-js": "dev-master",
        "awema-pl/module-navigation": "dev-master",
        "awema-pl/module-tab-builder": "dev-master",
        "awema-pl/module-table-builder": "dev-master",
        "awema-pl/module-context-menu": "dev-master",
        "awema-pl/module-localization-helper": "dev-master",
        "illuminate/support": "~5|~6|~7|~8|~9",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "AwemaPL\\IndigoLayout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AwemaPL\\IndigoLayout\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwemaPL\\IndigoLayout\\IndigoLayoutServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Theme-switcher",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../System-notify",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../Base-js",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../Navigation",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../Tab-builder",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../Context-menu",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../Localization-helper",
            "options": {
                "symlink": true
            }
        },        {
            "type": "path",
            "url": "../Table-builder",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
